<p id="notice"><%= notice %></p>

<h1>Rental Lines</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Rental Item</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rental_lines.each do |rental_line| %>
      <tr>
        <td><%= rental_line.order_id %></td>
        <td><%= rental_line.rental_item.rental_item_name %></td>
        <td><%= link_to 'Show', rental_line %></td>
        <td><%= link_to 'Edit', edit_rental_line_path(rental_line) %></td>
        <td><%= link_to 'Destroy', rental_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rental Line', new_rental_line_path %>
