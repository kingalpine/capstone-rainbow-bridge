<p id="notice"><%= notice %></p>

<h1>Order Lines</h1>

<table>
  <thead>
    <tr>
      <th>Order line description</th>
      <th>Order line start date</th>
      <th>Order line finish date</th>
      <th>Special order notes</th>
      <th>Product</th>
      <th>Order line status</th>
      <th>Order</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_lines.each do |order_line| %>
      <tr>
        <td><%= order_line.order_line_description %></td>
        <td><%= order_line.order_line_start_date %></td>
        <td><%= order_line.order_line_finish_date %></td>
        <td><%= order_line.special_order_notes %></td>
        <td><%= order_line.product_id %></td>
        <td><%= order_line.order_line_status_id %></td>
        <td><%= order_line.order_id %></td>
        <td><%= order_line.Image %></td>
        <td><%= link_to 'Show', order_line %></td>
        <td><%= link_to 'Edit', edit_order_line_path(order_line) %></td>
        <td><%= link_to 'Destroy', order_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order Line', new_order_line_path %>
