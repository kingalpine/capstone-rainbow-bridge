<p id="notice"><%= notice %></p>

<h1>Test Orders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_orders.each do |test_order| %>
      <tr>
        <td><%= test_order.name %></td>
        <td><%= test_order.notes %></td>
        <td><%= link_to 'Show', test_order %></td>
        <td><%= link_to 'Edit', edit_test_order_path(test_order) %></td>
        <td><%= link_to 'Destroy', test_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test Order', new_test_order_path %>
